<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:width="@dimen/list_icon_size"
            android:height="@dimen/list_icon_size"
            android:shape="rectangle">
            <solid android:color="?attr/background" />
            <corners android:radius="@dimen/list_icon_corner_radius" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/margin_small"
        android:gravity="center"
        android:top="@dimen/margin_small">
        <bitmap
            android:src="@mipmap/cells"
            android:tint="?attr/colorControlNormal" />
    </item>
</layer-list>
